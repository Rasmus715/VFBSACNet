<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@android:color/holo_red_light">

    <Button
        android:id="@+id/signOut_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/personalFragmentName"
        android:layout_alignEnd="@+id/personalFragmentName"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="-116dp"
        android:text="Sign Out"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/personalFragmentName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Personal Fragment"
        android:textSize="30sp"
        android:layout_centerInParent="true">
    </TextView>

    <TextView
        android:id="@+id/user_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/personalFragmentName"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="182dp"
        android:layout_marginBottom="188dp"
        tools:text="name " />

    <TextView
        android:id="@+id/group_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/personalFragmentName"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="156dp"
        tools:text="groupName " />

</RelativeLayout>